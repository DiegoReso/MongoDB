<!DOCTYPE html>
<html lang="pt-br">
<head>
  <%-include ('partials/head.ejs')  %>
</head>
<body>

  <h1>Lista</h1>

  <%-include ('partials/nav.ejs')  %>


  <ul>
    <% users.forEach((user) => {%>
      <li>
        Nome: <%= user.name%> - E-mail:<%= user.email%> - Idade: <%= user.age%> - <a href="/edit?id=<%= user._id %>">[editar]</a> - <a href="/remove/<%= user._id %>">[remover]</a>
      </li>  

    <%})  %>
  </ul>

  <%-include ('partials/foot.ejs')  %>
  

</body>
</html>